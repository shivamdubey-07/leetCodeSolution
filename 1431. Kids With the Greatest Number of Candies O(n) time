class Solution {
    public List<Boolean> kidsWithCandies(int[] candies, int extraCandies) {

        List<Boolean> l1=new ArrayList<>();

        int max=0;
        for(int j=0;j<candies.length;j++){

            if(candies[j]>max){
                max=candies[j];
            }
                   
                   
                }
        
        
            int count=0;
        for(int i=0;i<candies.length;i++){
                int sum=candies[i]+extraCandies;

               
               if(sum>=max){
                   l1.add(true);

               }
               else{
                   l1.add(false);
               }



            
        }
        return l1;

        

        
    }
}
